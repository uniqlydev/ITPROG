<div class="button-menu-container">
    <ul>
    <% foods.forEach((food, index) => { %>
        <% if (food.type === "D") { %>
        <li>
            <div class="item">
                <img src="<%= food.image %>" alt="<%= food.name %>" class="pic">
            </div>
            <div>
                <p class="label"><%= food.name %></p>
                <div class="cal-content">
                    <img src="/images/cart/calories.png" class="calorie-intake">
                    <p class="calorie-1"><%= food.protein %>g</p>
                </div>
                <p class="desc"><%= food.description %></p>
                <br>
            </div>
            <div class="buy">
                <p>PHP <%= food.price %></p>
                <button class="button-menu" id="drinks<%= index + 1 %>"></button>
            </div>
        </li>
        <% } %>
    <% }); %>
    </ul>
</div>


